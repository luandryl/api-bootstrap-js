{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["./routes/", "./geometrics/"],
    "exclude": [ "node_modules/", "./build", "./docs" ]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "name": "Area API Docs",
    "footerText": "luandryl@gmail.com"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "node_modules/tui-jsdoc-template"
  }
}